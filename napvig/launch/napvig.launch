	<node type="napvig" name="napvig" pkg="napvig" output="screen" />


	<!-- Napvig-X -->
	<include file="$(find target_detection)/launch/target_detection.launch" />

	<!-- Vehicle control -->
	<include file="$(find unicycle_control)/launch/unicycle_control.launch" />
</launch>